\begin{MHint}{L\"osung}
\[\frac{20  y^4  z^2 + 12  y^4}{ - 4  y^4  z - 12  y^4}=\frac{(4  y^4)\cdot(5  z^2 + 3)}{(4  y^4)\cdot( - z - 3)}=\frac{5  z^2 + 3}{ - z - 3}=\frac{ - 5  z^2 - 3}{z + 3}\:.\]\end{MHint}
 \end{MExercise}
\begin{MExercise}
K"urzen Sie soweit m"oglich: \MEquationItem{$\displaystyle \frac{5  y^5  z^3}{- 7  y^5  z^3}$}{\MLSimplifyQuestion{21}{(-5)/7}{5}{}{5}{528}{AUTOGENERATED}}\: .\\ 
\begin{MHint}{L\"osung}
\[\frac{5  y^5  z^3}{- 7  y^5  z^3}=\frac{(y^5  z^3)\cdot5}{(y^5  z^3)\cdot(-7)}=\frac{5}{-7}=\frac{-5}{7}\:.\]\end{MHint}
 \end{MExercise}
\begin{MExercise}
K"urzen Sie soweit m"oglich: \MEquationItem{$\displaystyle \frac{32  y^4 - 32  y^4  z^2}{56  y^4 - 8  y^4  z^3}$}{\MLSimplifyQuestion{39}{(4*z^2-4)/(z^3-7)}{5}{z}{5}{528}{AUTOGENERATED}}\: .\\ 
\begin{MHint}{L\"osung}
\[\frac{32  y^4 - 32  y^4  z^2}{56  y^4 - 8  y^4  z^3}=\frac{(8  y^4)\cdot(4 - 4  z^2)}{(8  y^4)\cdot(7 - z^3)}=\frac{4 - 4  z^2}{7 - z^3}=\frac{4  z^2 - 4}{z^3 - 7}\:.\]\end{MHint}
 \end{MExercise}
